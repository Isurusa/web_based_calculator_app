{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"CalculatorApp\",\n  data() {\n    return {\n      currentInput: \"\",\n      calculation_method: \"\",\n      calculation_result: \"\"\n    };\n  },\n  methods: {\n    appendToInput(value) {\n      this.currentInput += value;\n\n      //console.log(this.currentInput);\n    },\n    clearInput() {\n      this.currentInput = \"\";\n    },\n    toggleSign() {\n      if (this.currentInput.startsWith(\"-\")) {\n        this.currentInput = this.currentInput.substring(1);\n      } else if (this.currentInput) {\n        this.currentInput = \"-\" + this.currentInput;\n      }\n\n      //console.log(this.currentInput);\n    },\n    calculatePercentage() {\n      if (this.currentInput) {\n        //console.log(this.currentInput);\n\n        this.calculation_method = this.currentInput + \"%\";\n        this.currentInput = (parseFloat(this.currentInput) / 100).toString();\n        this.calculation_result = this.currentInput;\n        this.sendToStore();\n\n        //console.log(this.currentInput);\n      }\n    },\n    calculateResult() {\n      try {\n        //console.log(this.currentInput);\n\n        this.calculation_method = this.currentInput;\n        this.currentInput = eval(this.calculation_method).toString();\n        this.calculation_result = this.currentInput;\n\n        //console.log(this.currentInput);\n        this.sendToStore();\n      } catch (e) {\n        this.currentInput = \"Error\";\n      }\n    },\n    async sendToStore() {\n      console.log(\"yyyy\");\n      console.log(this.calculation_method);\n      console.log(this.calculation_result);\n      let formData = new FormData();\n      formData.append(\"calculation_method\", this.calculation_method);\n      formData.append(\"calculation_result\", this.calculation_result);\n      let url = \"http://127.0.0.1:8000/api/calculation_store\";\n      await axios.post(url, formData).then(respo
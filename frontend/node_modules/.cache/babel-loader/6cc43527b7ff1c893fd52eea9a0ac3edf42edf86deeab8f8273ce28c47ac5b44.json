rentInput) {\n        //console.log(this.currentInput);\n\n        this.calculation_method = this.currentInput + \"%\";\n        this.currentInput = (parseFloat(this.currentInput) / 100).toString();\n        this.calculation_result = this.currentInput;\n\n        this.sendToStore();\n\n        //console.log(this.currentInput);\n      }\n    },\n    calculateResult() {\n      try {\n        //console.log(this.currentInput);\n\n        this.calculation_method = this.currentInput;\n        this.currentInput = eval(this.calculation_method).toString();\n        this.calculation_result = this.currentInput;\n\n        //console.log(this.currentInput);\n        this.sendToStore();\n      } catch (e) {\n        this.currentInput = \"Error\";\n      }\n    },\n    async sendToStore() {\n      console.log(\"yyyy\");\n      console.log(this.calculation_method);\n      console.log(this.calculation_result);\n\n      let formData = new FormData();\n      formData.append(\"calculation_method\", this.calculation_method);\n      formData.append(\"calculation_result\", this.calculation_result);\n\n      let url = \"http://127.0.0.1:8000/api/calculation_store\";\n\n      await axios\n        .post(url, formData)\n        .then((response) => {\n          // console.log(response);\n\n          console.log(response);\n\n          if (response.data.status == 200) {\n            console.log(response.data.message);\n          } else {\n            console.log(response.data.message);\n          }\n        })\n        .catch((error) => {\n          this.errors.push(error.response);\n        });\n    },\n  },\n};\n</script>\n\n<style scoped>\n.calculator {\n  width: 320px;\n  margin: 50px auto;\n  padding: 20px;\n  border: 1px solid #ccc;\n  border-radius: 10px;\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);\n  background: #000;\n  color: #fff;\n}\n.display {\n  margin-bottom: 10px;\n}\n.display input {\n  width: 100%;\n  height: 60px;\n  font-size: 32px;\n  text-align: right;\n  padding-right: 